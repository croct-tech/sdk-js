    }
  },
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/croct-tech/sdk-js.git"
  },
  "bugs": {
    "url": "https://github.com/croct-tech/sdk-js/issues"
  },
  "homepage": "https://github.com/croct-tech/sdk-js#readme",
  "scripts": {
