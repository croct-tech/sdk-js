    "test": "jest -c jest.config.js --coverage",
    "validate": "tsc --noEmit",
    "build": "tsup",
    "postbuild": "./post-build.js"
  },
  "dependencies": {
    "@croct/json": "^2.0.1",
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "@croct/eslint-plugin": "^0.7.0",
    "@types/jest": "^29.2.3",
    "eslint": "^8.57.0",
    "fetch-mock": "^9.11.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-extended": "^4.0.0",
    "node-fetch": "^2.6.7",
    "ts-jest": "^29.0.3",
    "tsup": "^8.4.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "**/*.mjs",
