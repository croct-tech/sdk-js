{
  "$schema": "http://json-schema.org/schema#",
  "$id": "https://schema.croct.io/web/cart.json",
  "type": "object",
  "properties": {
    "cartId": {
      "type": "string"
    },
    "currency": {
      "type": "string"
    },
    "items": {
      "type": "array",
      "items": {
        "$ref": "https://schema.croct.io/web/cart-item.json"
      }
    },
    "subtotal": {
      "type": "number"
    },
    "shippingPrice": {
      "type": "number"
    },
    "taxes": {
      "type": "object",
      "additionalProperties": {
        "type": "number"
      }
    },
    "costs": {
      "type": "object",
      "additionalProperties": {
        "type": "number"
      }
    },
    "discount": {
      "type": "number"
    },
    "total": {
      "type": "number"
    },
    "coupon": {
      "type": "string"
    },
    "lastUpdateTime": {
      "type": "integer"
    }
  },
  "required": [
    "currency",
    "items",
    "total",
    "lastUpdateTime"
  ]
}